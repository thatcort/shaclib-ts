    "@types/gulp-typescript": "^2.13.0",
    "@types/merge2": "^0.3.30",
    "@types/mocha": "^2.2.41",
    "@types/n3": "0.0.2",
    "@types/node": "^7.0.13",
    "@types/run-sequence": "0.0.29",
    "@types/sinon": "^2.1.2",
    "@types/sinon-as-promised": "^4.0.6",
    "@types/superagent": "^2.0.36",
    "@types/uuid": "^2.0.29",
    "babel-core": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-reexport": "^0.1.21",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-tslint": "^8.0.0",
    "gulp-typescript": "^3.1.6",
    "merge2": "^1.0.3",
    "mocha": "^3.2.0",
    "nyc": "9.0.1",
    "run-sequence": "^1.2.2",
    "sinon": "^2.1.0",
    "sinon-as-promised": "^4.0.3",
    "source-map-support": "^0.4.14",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "cli": "^1.0.1",
    "cli-color": "^1.2.0",
    "clui": "^0.3.1",
    "rdflib-ts": "^1.0.2",
    "superagent": "^3.5.2",
    "uuid": "^3.0.1"
