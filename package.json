  "description": "TypeScript implementation of the evolving W3C Shapes Constraint Language (SHACL)",
  "homepage": "https://bitbucket.org/vladimir_djurdjevic/shaclib.ts",
  "keywords": [
    "rdf",
    "sparql",
    "shacl"
  ],
  "author": "Vladimir Djurdjevic <vladimirdjurdjevic93@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/vladimir_djurdjevic/shaclib.ts/issues",
    "email": "vladimirdjurdjevic93@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://vladimir_djurdjevic@bitbucket.org/vladimir_djurdjevic/shaclib.ts.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "shacl-cli": "lib/cli/shacl-cli.js"
  },
  "scripts": {
    "tsc": "tsc",
    "gulp": "gulp",
    "lint": "npm run gulp lint",
    "clean": "npm run gulp clean",
    "clean:lib": "npm run gulp clean-lib",
    "clean:coverage": "npm run gulp clean-coverage",
    "copy:lib": "npm run gulp copy-lib",
    "copy:datasets": "npm run gulp copy-datasets",
    "compile": "npm run tsc && npm run copy:datasets",
    "reexport": "npm run gulp reexport",
    "build": "npm run gulp build",
    "link": "npm run build && npm link && npm link shaclib-ts",
    "publish": "npm run gulp publish",
    "test:unit": "nyc mocha test/unit/**/*.ts",
    "test:integration": "nyc mocha test/integration/**/*.ts --timeout 10000",
    "test:e2e": "nyc mocha test/e2e/**/*.ts --timeout 10000",
    "test": "nyc mocha test/**/*.ts --timeout 10000"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
